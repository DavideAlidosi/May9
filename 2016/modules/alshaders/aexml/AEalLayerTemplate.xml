			<label>Indirect diffuse</label>
			<description></description>
		</attribute>
		<attribute name="aov_indirect_diffuse_raw" type="maya.float3">
			<label>Indirect diffuse (raw)</label>
			<description></description>
		</attribute>
		<attribute name="aov_direct_backlight" type="maya.float3">
			<label>Direct backlight</label>
			<description></description>
		</attribute>
		<attribute name="aov_indirect_backlight" type="maya.float3">
