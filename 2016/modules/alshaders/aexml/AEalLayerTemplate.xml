<?xml version="1.0" encoding="UTF-8"?>
	<templates>
	<template name="AEalLayer">
		<label>alLayer</label>
		<description>Simply blends two surface shaders, including all their AOVs while preserving light group information.</description>
		<attribute name="layer1" type="maya.float3">
			<label>Layer 1</label>
			<description>The bottom layer to blend.</description>
		</attribute>
		<attribute name="layer2" type="maya.float3">
			<label>Layer 2</label>
			<description>The top layer to blend.</description>
		</attribute>
		<attribute name="mix" type="maya.float">
			<label>Mix</label>
			<description>The amount to blend from the bottom layer to the top.</description>
		</attribute>
		<attribute name="debug" type="maya.enum">
			<label>Debug</label>
			<description>Allows you to quickly preview each layer and the mix value.</description>
		</attribute>
		<attribute name="standardCompatibleAOVs" type="maya.bool">
			<label>Write standard AOVs only</label>
			<description></description>
		</attribute>
		<attribute name="aov_diffuse_color" type="maya.float3">
			<label>Diffuse color</label>
			<description></description>
		</attribute>
		<attribute name="aov_direct_diffuse" type="maya.float3">
			<label>Direct diffuse</label>
			<description></description>
		</attribute>
